{
  "name": "react-cordova",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.2",
    "cordova-android": "^8.1.0",
    "cordova-custom-config": "^5.1.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "es6-promise-plugin": "^4.2.2",
    "install": "^0.13.0",
    "ionicons": "^5.0.0",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "npm": "^6.13.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-masonry-css": "^1.0.14",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-slick": "^0.25.2",
    "react-sliding-side-panel": "^1.0.13",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "slick-carousel": "^1.8.1",
    "typescript": "3.7.4",
    "uninstall": "0.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:android": "cordova build android --release -- --packageType=bundle --password=$ATGPASS --storePassword=$ATGSTOREPASS",
    "build-prod": "ionic build --prod --engine=cordova --platform=android",
    "build-ios": "cordova build ios --prod --release --emulator",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "emulator:android": "cordova run android",
    "emulator:ios": "cordova run ios",
    "simulate:android": "simulate android --target=chrome",
    "simulate:ios": "simulate ios --target=chrome",
    "build ios": "ionic build && cordova build ios --prod --release",
    "build:ionic": "ionic build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "An Ionic project",
  "devDependencies": {
    "@types/react-redux": "^7.1.7",
    "@types/react-slick": "^0.23.4",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-splashscreen": "5.0.2",
    "cordova-plugin-statusbar": "2.4.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.0.1",
    "node-sass": "^4.13.1",
    "prettier": "^1.16.4",
    "react-styleguidist": "^11.0.5",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "why-did-you-update": "^1.0.8"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-custom-config": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-screen-orientation": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "coverageReporters": [
      "json",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ]
  }
}
